{
  "name": "react-relay",
  "version": "1.0.0",
  "description": "An Electron application with React and TypeScript",
  "main": "./dist/main/index.js",
  "author": "example.com",
  "homepage": "https://www.electronjs.org",
  "scripts": {
    "prebuild": "electron-vite build",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-forge start",
    "server": "ts-node server/server.ts",
    "dev": "electron-vite dev --watch",
    "both": "concurrently --kill-others \"npm run server\" \"npm run dev\"",
    "build": "npm run typecheck && electron-vite build --outDir=dist",
    "make": "electron-vite build --outDir=dist && electron-forge make",
    "package": "electron-vite build --outDir=dist && electron-forge package"
  },
  "dependencies": {
    "@babel/parser": "^7.22.10",
    "@babel/traverse": "^7.22.10",
    "@electron-forge/maker-deb": "^6.4.1",
    "@electron-forge/maker-dmg": "^6.4.1",
    "@electron-forge/maker-rpm": "^6.4.1",
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^1.0.2",
    "@reduxjs/toolkit": "^1.9.5",
    "cors": "^2.8.5",
    "dagre": "^0.8.5",
    "electron-updater": "^5.3.0",
    "elkjs": "^0.8.2",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "prismjs": "^1.29.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0",
    "reactflow": "^11.8.2",
    "rollup-plugin-copy": "^3.5.0",
    "web-worker": "^1.2.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.4.1",
    "@electron-forge/maker-squirrel": "^6.4.1",
    "@electron-forge/maker-zip": "^6.4.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@types/node": "^18.16.19",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.15",
    "concurrently": "^8.2.0",
    "daisyui": "^3.5.1",
    "electron": "^24.8.3",
    "electron-vite": "^1.0.27",
    "esbuild": "^0.19.2",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "postcss": "^8.4.27",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.2"
  }
}
